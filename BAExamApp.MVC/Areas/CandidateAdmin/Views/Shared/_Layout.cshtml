<script>
    function handleLogout() {
        localStorage.removeItem('selectedAccordionMenu');
        localStorage.removeItem('selectedMenuItem');
        sessionStorage.removeItem('sessionActive');

    }

    window.addEventListener('beforeunload', function () {
        startLoader();
    });

    window.addEventListener("load", function () {
        stopLoader();
    });

    function handleAccordionMenuClick(menuName) {
        localStorage.setItem('selectedAccordionMenu', menuName);
        sessionStorage.setItem('sessionActive', 'true');

        var clickedMenu = document.getElementById(menuName + 'AccordionMenu');

        if (clickedMenu) {
            document.querySelectorAll('.menu-item.menu-accordion').forEach(function (menu) {
                menu.classList.remove('active', 'show', 'hover');
            });

            clickedMenu.classList.add('active', 'show', 'hover');
        }
    }

    function handleMenuItemClick(menuItemName) {
        localStorage.setItem('selectedMenuItem', menuItemName);
        sessionStorage.setItem('sessionActive', 'true');

        var clickedMenuItem = document.getElementById(menuItemName);

        if (clickedMenuItem) {
            document.querySelectorAll('.menu-item').forEach(function (item) {
                item.classList.remove('active', 'show', 'hover');
            });

            clickedMenuItem.classList.add('show', 'active', 'hover');

            var accordionMenu = clickedMenuItem.closest('.menu-item.menu-accordion');
            if (accordionMenu) {
                accordionMenu.classList.add('show', 'hover', 'active');
            }
        }
    }

    document.addEventListener("DOMContentLoaded", function () {
        var logoElement = document.querySelector('a[href="/CandidateAdmin"]');
        if (logoElement) {
            logoElement.addEventListener('click', function () {
                document.querySelectorAll('.menu-item, .menu-item.menu-accordion').forEach(function (item) {
                    item.classList.remove('show', 'active', 'hover');
                });

                localStorage.removeItem('selectedAccordionMenu');
                localStorage.removeItem('selectedMenuItem');
                sessionStorage.removeItem('sessionActive');
            });
        }

        if (!sessionStorage.getItem('sessionActive')) {
            localStorage.removeItem('selectedAccordionMenu');
            localStorage.removeItem('selectedMenuItem');
        } else {
            var selectedMenuItem = localStorage.getItem('selectedMenuItem');
            if (selectedMenuItem) {
                var menuItem = document.getElementById(selectedMenuItem);
                if (menuItem) {
                    menuItem.classList.add('show', 'active', 'hover');
                    var accordionMenu = menuItem.closest('.menu-item.menu-accordion');
                    if (accordionMenu) {
                        accordionMenu.classList.add('show', 'hover', 'active');
                    }
                }
            }

            var selectedAccordionMenu = localStorage.getItem('selectedAccordionMenu');
            if (selectedAccordionMenu) {
                var accordionMenu = document.getElementById(selectedAccordionMenu);
                if (accordionMenu) {
                    accordionMenu.classList.add('show', 'hover', 'active');
                }
            }
        }
    });
</script>
